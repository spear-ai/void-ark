{
  "name": "@void-ark/rust-base",
  "version": "1.1.0",
  "description": "Rust base image with architecture support for offline development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cargo build --release",
    "dev": "cargo run",
    "test": "cargo test",
    "lint": "cargo clippy -- -D warnings",
    "format": "cargo fmt",
    "format:check": "cargo fmt -- --check",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "clean": "cargo clean && rm -rf dist",
    "dockerfilelint": "dockerfilelint Dockerfile"
  },
  "devDependencies": {
    "@types/node": "20.0.0",
    "dockerfilelint": "1.8.0",
    "prettier": "3.1.0",
    "typescript": "5.8.3"
  },
  "keywords": [
    "base-image",
    "docker",
    "rust",
    "performance",
    "utilities",
    "void-ark"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.8.1"
}
